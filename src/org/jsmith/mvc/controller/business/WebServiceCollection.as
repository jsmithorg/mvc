package org.jsmith.mvc.controller.business{	import flash.utils.Dictionary;		import mx.rpc.soap.WebService;		import org.jsmith.mvc.error.MVCError;		public class WebServiceCollection extends ServiceCollection implements IServiceCollection	{		public function addService(service:IServiceDefinition):void 		{			_serviceDefinitions[service.serviceID] = service;						var webService:WebService = new WebService();			webService.destination = service.serviceID;			webService.useProxy = false;			webService.loadWSDL(service.url);						_services[service.serviceID] = webService;					}//end method				public override function getService(serviceID:String):Object		{			var service:WebService = _services[serviceID];						if(service == null)				throw new MVCError(MVCError.SERVICE_NOT_FOUND, serviceID);							return service;					}//end method				public override function logout():void 		{			for(var serviceID:String in _services)         	{				var service:WebService = _services[serviceID];				service.logout();         				}//end for					}//end method			}//end class	}//end package