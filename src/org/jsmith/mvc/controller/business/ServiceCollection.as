package org.jsmith.mvc.controller.business{	import flash.utils.*;		import org.jsmith.mvc.controller.business.*;	import org.jsmith.mvc.error.MVCError;		public class ServiceCollection implements IServiceCollection	{		protected var _services:Dictionary;		protected var _serviceDefinitions:Dictionary;				public function ServiceCollection()		{			_services = new Dictionary();			_serviceDefinitions = new Dictionary();					}//end method				public function register(service:IServiceRegister):void		{			throw new MVCError(MVCError.ABSTRACT_METHOD_ERROR);					}//end method				public function getService(serviceID:String):Object		{			throw new MVCError(MVCError.ABSTRACT_METHOD_ERROR);					}//end method				public function setCredentials(username:String, password:String):void		{			throw new MVCError(MVCError.ABSTRACT_METHOD_ERROR);					}//end method				public function setRemoteCredentials(username:String, password:String):void		{			throw new MVCError(MVCError.ABSTRACT_METHOD_ERROR);					}//end method				public function logout():void		{			throw new MVCError(MVCError.ABSTRACT_METHOD_ERROR);					}//end method		 		protected function getAccessors(service:IServiceRegister):XMLList		{		 var description:XML = describeType(service);		 var accessors:XMLList = description.accessor.(@access == "readwrite").@name;					 return accessors;		 		}//end method	  	}//end class	}//end package