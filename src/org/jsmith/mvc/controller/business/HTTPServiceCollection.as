package org.jsmith.mvc.controller.business{	import flash.utils.Dictionary;		import mx.rpc.http.HTTPService;		import org.jsmith.mvc.error.MVCError;		public class HTTPServiceCollection extends ServiceCollection implements IServiceCollection	{		public function addService(service:IServiceDefinition):void 		{			_serviceDefinitions[service.serviceID] = service;						var sd:HTTPServiceDefinition = service as HTTPServiceDefinition;						var httpService:HTTPService = new HTTPService();			//httpService.destination = service.serviceID;			httpService.url = sd.url;			httpService.method = sd.method;			httpService.resultFormat = sd.resultFormat;						_services[service.serviceID] = httpService;					}//end method				public override function getService(serviceID:String):Object		{			var service:HTTPService = _services[serviceID];						if(service == null)				throw new MVCError(MVCError.SERVICE_NOT_FOUND, serviceID);							return service;					}//end method				public override function logout():void 		{			for(var serviceID:String in _services)         	{				var service:HTTPService = _services[serviceID];				service.logout();         				}//end for					}//end method			}//end class	}//end package